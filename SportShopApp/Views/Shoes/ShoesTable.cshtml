@using SportShopApp.Models;
<link rel="stylesheet" href="~/Content/ShoesTable.css" />

<table class="shoesTable">

    <tr>
        <th>Id</th>
        <th>Shoe Type</th>
        <th>Company</th>
        <th>Model</th>
        <th>Price</th>
        <th>Quantity</th>
    </tr>
    @foreach (Shoe shoe in Model)
    {
        <tr>
            <td>@shoe.Id</td>
            <td>@shoe.ShoeType</td>
            <td>@shoe.Company</td>
            <td>@shoe.Model</td>
            <td>@shoe.Price$</td>
            <td>@shoe.Quantity</td>
        </tr>

    }

</table>
